            } else {
                if (action.selectionIndependent) {
                    action.run();
                } else {
                    thiz.getSelectedItems(action.run, true);
                }
            }
        },
        getGroup: function () {
            return (typeof action.group == "function" ? action.group() : action.group) || "No group";
