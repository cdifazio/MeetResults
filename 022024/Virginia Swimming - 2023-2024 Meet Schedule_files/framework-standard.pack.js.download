    var thiz = this;
    return {
        isVisible: function () {
            if (typeof(action.visible) == "undefined") {
                return true;
            } else {
                if (typeof(action.visible) == "function") return action.visible(thiz.getSelectedItemCount(), thiz);
                return action.visible;
