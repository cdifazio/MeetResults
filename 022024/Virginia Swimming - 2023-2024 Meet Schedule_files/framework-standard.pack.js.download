        thiz.items = results || [];
        thiz.totalItems = totalItems || thiz.items.length;
        thiz._groupItemsByDate(thiz.items);
        if (thiz.options && thiz.options.onDataLoaded) {
            thiz.options.onDataLoaded(results, thiz.totalItems);
        }
        callback();
