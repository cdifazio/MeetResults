    return Dom.newDOMElement({
        _name: "hbox",
        "class": "Item",
        tabindex: 0,
        _children: [
            {_name: "span", _text: this.renderer(item, true)},
            {_name: "icon", "class": "close-circle CloseIcon"}
        ]
    });
