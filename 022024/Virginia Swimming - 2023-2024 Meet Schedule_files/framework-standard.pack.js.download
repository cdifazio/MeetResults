    for (var i = 0; i < this.options.length; i++) {
        var option = this.options[i];
        if (option.role == SelectBox.OPTION_ROLE_HANDLE) {
            if (option.type == SelectBox.HANDLE_TYPE_SELECTALL) {
                if (this.multipleSelect) {
