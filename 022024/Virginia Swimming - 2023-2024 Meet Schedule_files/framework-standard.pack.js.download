                },
                handler: function (item) {
                    //console.log("Remove ", item);
                    var foundIndex = -1;
                    var cb = function(selectedsInPaginator, thiz) {
                        for (var index = 0; index < selectedsInPaginator.length; index++) {
                            if (thiz.comparer(selectedsInPaginator[index], item)) {
                               foundIndex = index;
                               break;
