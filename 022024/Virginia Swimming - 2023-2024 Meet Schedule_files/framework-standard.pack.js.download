    function javaToMomentFormat(javaFormat) {
        var momentFormat = javaFormat.replace(/([^ :\/\,\-T]+)/g, function (zero, term) {
            if (term == "dd") return "DD";
            if (term == "d") return "D";
            if (term == "y") return "Y";
